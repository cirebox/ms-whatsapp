{
  "name": "ms-whatsapp",
  "version": "1.0.0",
  "description": "WhatsApp API usando Fastify e TypeScript",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write 'src/**/*.{ts,js,json}'",
    "format:check": "prettier --check 'src/**/*.{ts,js,json}'",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "whatsapp",
    "api",
    "fastify",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/static": "^6.10.2",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.9.3",
    "@fastify/view": "^8.0.0",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "fastify": "^4.20.0",
    "fastify-plugin": "^4.5.0",
    "openai": "^4.0.0",
    "pino": "^8.14.1",
    "pino-pretty": "^10.2.0",
    "whatsapp-web-webpack-exodus.js": "github:arktnld/whatsapp-web-webpack-exodus.js"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.4.5",
    "@types/ejs": "3.1.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}